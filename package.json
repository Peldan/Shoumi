{
  "name": "Shoumi",
  "version": "1.0.2",
  "description": "An image viewer with networking",
  "main": "js/index.js",
  "dependencies": {
    "electron-is-dev": "^0.3.0",
    "electron-log": "^2.2.17",
    "electron-prompt": "^1.1.0-1",
    "electron-updater": "^3.1.2",
    "jquery": "^3.3.1",
    "materialize-css": "^1.0.0-rc.2",
    "nodejs": "0.0.0",
    "peer": "^0.2.10",
    "pg": "^7.4.3",
    "simple-peer": "^9.1.2",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "build": {
    "appId": "com.example.shoumi",
    "win": {
      "target": "nsis",
      "publish": [
        {
          "provider": "github"
        }
      ],
      "extraResources": {
        "from": "./build/trayicon.png",
        "from": "./build/icon.ico",
        "to": "."
      }
    }
  },
  "devDependencies": {
    "electron": "2.0.8",
    "electron-builder": "^20.28.4",
    "electron-builder-squirrel-windows": "^20.28.3",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
  },
  "scripts": {
    "start": "electron .",
    "publish": "build --win -p always",
    "dist": "electron-builder",
    "debug": "set DEBUG=electron-builder && build",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Peldan/imgviewer.git"
  },
  "standard": {
    "globals": [
      "$"
    ]
  },
  "keywords": [],
  "author": "Arvid Peld√°n",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Peldan/imgviewer/issues"
  },
  "homepage": "https://github.com/Peldan/imgviewer#readme"
}
